  <script>
    $(document).foundation();
    $(document).ready(function () {
      $('#site-row').css('left', '-100%');
      
      var animationDuration = 366,
          lastPage = null,
          newPage = null,
          bgColor,
          inTransition = false,
          lastPage = 2,
          currentPage = 2;
      
      function transitionOnClickOn (buttonElt, leftScroll) {
        /*
        newPage = buttonElt.parent().parent();
        
        if (lastPage) {
          lastPage.animate({
            backgroundColor: bgColor
          }, animationDuration);
        }
        
        bgColor = newPage.css('backgroundColor');
        
        newPage.animate({
          backgroundColor: "#424242"
        }, animationDuration);
        */
        $('#site-row').animate({
          left: leftScroll
        }, {
          duration: animationDuration,
          easing: 'linear'
        });
        /*
        lastPage = newPage;
        */
      }
      
      function simpleTransition (leftScroll) {
        if (!inTransition) {
          inTransition = true;
          setTimeout(function () {
            inTransition = false;
          }, 1000);
          
          $('#site-row').animate({
            left: leftScroll
          }, {
            duration: animationDuration,
            easing: 'easeOutCirc',
            complete: function () {
              if (leftScroll.slice(0, 1) == "+") {
                currentPage--;
              }
              else {
                currentPage++;
              }
            }
          });
        }
      }
      
      $('.page').mousewheel(function (e) {
        if (e.deltaY < 0 && currentPage < lastPage) {
          simpleTransition("-=100%");
        }
        if (e.deltaY > 0 && currentPage > 1) {
          simpleTransition("+=100%");
        }
      });
      
      $('.move-right').click(function () {
        //transitionOnClickOn($(this), "-=100%");
        simpleTransition("-=100%");
      });
      
      $('.move-left').click(function () {
        //transitionOnClickOn($(this), "+=100%");
        simpleTransition("+=100%");
      });
    });
  </script>
